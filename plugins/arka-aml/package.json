{
  "name": "@arka/aml",
  "version": "1.0.0",
  "description": "Anti-Money Laundering compliance domain plugin for ARKA Protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
